<launch>

	<node pkg="phins" type="phins_publisher" name="phins" />
	
        <node pkg="truenorth" type="gyro_publisher" name="truenorth" output="screen">

               <param name="rate" value="10" type="int" />
	       <param name="hz" value="5000" type="int" />
	       <param name="port" value="/dev/ttyUSB0" type="str" />
	       <param name="baud" value="4147200" type="int" />
	       <param name="latitude" value="39.32" type="double" />
	       <param name="instr_align" value="1,0,0,0,-1,0,0,0,-1" type="string" />
	       <param name="gains" value="0.1,0.007,0.01,1.0,0.007,0.0,0.00000" type="string" />
	       <param name="log_loc" value="/home/spiels/log/imu_data/" type="string" />

        </node>

	<node pkg="rosbag" type="record" args="-o /home/spiels/log/rosbag/ /truenorth/gyro_data /phins/phins_data" name="record" output="screen"/>

</launch>
